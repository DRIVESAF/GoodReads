<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zzt.goodreads.mapper.UserMapper">
    <select id="selectByUserPhone" parameterType="String" resultType="com.zzt.goodreads.entity.User">
        SELECT *
        FROM user
        WHERE phone = #{value}


    </select>

    <insert id="insert" parameterType="com.zzt.goodreads.entity.User">
        INSERT INTO  user(
            phone,password
        )VALUES (
                    #{phone},

                    #{password},

                )
    </insert>
</mapper>